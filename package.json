{
  "name": "react-porfolio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build && npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Grigore94/react-porfolio.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Grigore94/react-porfolio/issues"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "raf": "^3.4.1",
    "react-dom": "^16.13.1",
    "react-mdl": "^2.1.0",
    "react-router-dom": "^5.2.0",
    "react-sizeme": "^2.6.12",
    "react-starfield-animation": "^1.0.1"
  },
  "homepage": "https://github.com/Grigore94/react-porfolio"
}
